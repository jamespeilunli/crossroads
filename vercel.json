{
  "builds": [
    {
      "src": "web/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "web/static/styles/index.css",
      "use": "@vercel/static"
    },
    {
      "src": "web/static/styles/tailwind.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web/app.py"
    }
  ]
}
